{
  "name": "stickynote",
  "version": "0.5.0",
  "description": "Creates collapsible client-side sticky notes",
  "keywords": [
    "sticky",
    "notes"
  ],
  "homepage": "https://github.com/brettz9/stickynote",
  "bugs": {
    "url": "https://github.com/brettz9/stickynote/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brettz9/stickynote.git"
  },
  "license": "MIT",
  "author": "Brett Zamir <brettz9@yahoo.com>",
  "contributors": [],
  "type": "module",
  "types": "./dist/index.d.cts",
  "exports": {
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.umd.js"
    }
  },
  "main": "src/index.js",
  "scripts": {
    "prepublishOnly": "npm run rollup",
    "attw": "attw --pack",
    "start": "open http://localhost:8017/demo/ && static -p 8017",
    "eslint": "eslint --ext=js,cjs,mjs,md,html .",
    "lint": "npm run eslint",
    "rollup": "echo '{\"type\": \"commonjs\"}' > dist/package.json && rollup -c && tsc"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@node-static/node-static": "^0.8.1",
    "eslint": "9.39.1",
    "eslint-config-ash-nazg": "39.8.0",
    "open-cli": "^8.0.0",
    "rollup": "^4.53.3"
  },
  "engines": {
    "node": "^20.11.0 || >=22.16.0"
  },
  "browserslist": [
    "last 2 chrome versions"
  ]
}
